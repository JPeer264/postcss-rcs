{
  "name": "postcss-rcs",
  "version": "2.0.1",
  "description": "The postcss plugin for rcs-core",
  "main": "build.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint index.js test.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run babel",
    "babel": "babel index.js -o build.js",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpeer264/postcss-rcs.git"
  },
  "keywords": [
    "postcss-plugin",
    "rcs",
    "postcss",
    "minify",
    "selectors",
    "rename",
    "munch",
    "css",
    "shrink",
    "classes",
    "compress",
    "styles"
  ],
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpeer264/postcss-rcs/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "homepage": "https://github.com/jpeer264/postcss-rcs#readme",
  "peerDependencies": {
    "postcss": "^7.x",
    "rcs-core": "^2.x"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.2",
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.2",
    "nyc": "^10.1.2",
    "postcss": "^8.2.10",
    "rcs-core": "^2.6.2"
  }
}
